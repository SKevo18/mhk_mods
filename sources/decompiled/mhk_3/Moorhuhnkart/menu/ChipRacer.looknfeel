	<!--
	***************************************************
	MHK/FrameWindow
	***************************************************
	-->
	
	<WidgetLook name="ChipRacer/FrameWindow">
	
		<Property name="NSSizingCursorImage" value="set:ChipRacer image:MouseArrow" />
		<Property name="EWSizingCursorImage" value="set:ChipRacer image:MouseArrow" />
		<Property name="NWSESizingCursorImage" value="set:ChipRacer image:MouseArrow" />
		<Property name="NESWSizingCursorImage" value="set:ChipRacer image:MouseArrow" />
		
		<NamedArea name="ClientWithTitleWithFrame">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" offset="0" type="RightEdge" />
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" offset="0" type="BottomEdge" />
				</Dim>
			</Area>
		</NamedArea>
		
		<NamedArea name="ClientWithTitleNoFrame">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">

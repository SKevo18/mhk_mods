<?xml version="1.0" ?>
<Falagard> 

	<!--
	***************************************************
	MHK/StaticImage
	***************************************************
	-->
	
	<WidgetLook name="ChipRacer/StaticImage">
		<Property name="FrameEnabled" value="False" />
		<Property name="BackgroundEnabled" value="False" />		
		
		<NamedArea name="WithFrameImageRenderArea">
			<Area>
				<Dim type="LeftEdge">
					<ImageDim imageset="ChipRacer" image="ThinLeftEdge" dimension="Width" />
				</Dim>
				<Dim type="TopEdge">
					<ImageDim imageset="ChipRacer" image="ThinTopEdge" dimension="Height" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge" />
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge" />
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="NoFrameImageRenderArea">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="Width">
					<UnifiedDim scale="1" type="Width" />
				</Dim>
				<Dim type="Height">
					<UnifiedDim scale="1" type="Height" />
				</Dim>
			</Area>
		</NamedArea>
		
		<ImagerySection name="bg">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="Background" imageset="ChipRacer" image="GenericBrush" />
				<VertFormat type="Tiled" />
				<HorzFormat type="Tiled" />
			</FrameComponent>
		</ImagerySection>		
		
		<StateImagery name="Enabled" />
		<StateImagery name="Disabled" />
		<StateImagery name="EnabledFrame" />
		<StateImagery name="DisabledFrame" />
		
		<StateImagery name="EnabledBackground" >
			<Layer>
				<Section section="bg">
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="DisabledBackground" >
			<Layer>
				<Section section="bg">
				</Section>
			</Layer>
		</StateImagery>
		
	</WidgetLook> 
	
	<!--
	***************************************************
	MHK/StaticText
	***************************************************
	-->
	<WidgetLook name="ChipRacer/StaticText">
		<Property name="FrameEnabled" value="False" />
		<Property name="BackgroundEnabled" value="False" />
		<NamedArea name="TextRenderArea">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<AbsoluteDim value="0" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<AbsoluteDim value="0" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="TextRenderAreaHScroll">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<AbsoluteDim value="5" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="TextRenderAreaVScroll">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<AbsoluteDim value="5" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="TextRenderAreaHVScroll">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="5" />
				</Dim>
				<Dim type="RightEdge">
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<WidgetDim widget="__auto_vscrollbar__" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="BottomEdge">
					<UnifiedDim scale="1" type="BottomEdge">
						<DimOperator op="Subtract">
							<WidgetDim widget="__auto_hscrollbar__" dimension="Height" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
			</Area>
		</NamedArea>
		<Child type="ChipRacer/HorizontalScrollbar" nameSuffix="__auto_hscrollbar__">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="Width">
					<UnifiedDim scale="1" offset="-15" type="Width" />
				</Dim>
				<Dim type="Height">
					<AbsoluteDim value="15" />
				</Dim>
			</Area>
			<VertAlignment type="BottomAligned" />
		</Child>
		<Child type="ChipRacer/VerticalScrollbar" nameSuffix="__auto_vscrollbar__">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="Width">
					<AbsoluteDim value="30" />
				</Dim>
				<Dim type="Height">
					<UnifiedDim scale="1" offset="-15" type="Height" />
				</Dim>
			</Area>
			<HorzAlignment type="RightAligned" />
		</Child>
		
		<ImagerySection name="bg">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="Background" imageset="ChipRacer" image="GenericBrush" />
				<VertFormat type="Tiled" />
				<HorzFormat type="Tiled" />
			</FrameComponent>
		</ImagerySection>

		<ImagerySection name="frame">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" offset="0" type="RightEdge" />
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" offset="0" type="BottomEdge" />
					</Dim>
				</Area>
				<Image type="TopLeftCorner" imageset="ChipRacer" image="GenericBrush" />
				<Image type="TopRightCorner" imageset="ChipRacer" image="GenericBrush" />
				<Image type="BottomLeftCorner" imageset="ChipRacer" image="GenericBrush" />
				<Image type="BottomRightCorner" imageset="ChipRacer" image="GenericBrush" />
				<Image type="LeftEdge" imageset="ChipRacer" image="GenericBrush" />
				<Image type="TopEdge" imageset="ChipRacer" image="GenericBrush" />
				<Image type="RightEdge" imageset="ChipRacer" image="GenericBrush" />
 				<Image type="BottomEdge" imageset="ChipRacer" image="GenericBrush" />
				<Image type="Background" imageset="ChipRacer" image="TransparentBrush" />			
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</FrameComponent>
		</ImagerySection>
		
		<StateImagery name="Enabled" >
		</StateImagery>
		
		<StateImagery name="Disabled" >
		</StateImagery>
		
		<StateImagery name="EnabledFrame" >
			<Layer>
				<Section section="frame">
					<Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="EnabledBackground" >
			<Layer>
				<Section section="bg">
					<Colours topLeft="7F00007F" topRight="7FFFFFFF" bottomLeft="7F0000FF" bottomRight="7FFFFFFF" />
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="DisabledFrame" />
		<StateImagery name="DisabledBackground" />
	</WidgetLook> 


	<!--
	***************************************************
	MHK/Button
	***************************************************
	-->
	
	<WidgetLook name="ChipRacer/Button">
	
		<Property name="NormalTextColour" value="FFFFFFFF" />
		<Property name="HoverTextColour" value="FFFFFFFF" />
		<Property name="PushedTextColour" value="FFFFFFFF" />
		<Property name="DisabledTextColour" value="FF555555" />
		
		<ImagerySection name="frame">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="LeftEdge" imageset="ChipRacer" image="ButtonLeftEdge" />
				<Image type="RightEdge" imageset="ChipRacer" image="ButtonRightEdge" />
				<Image type="TopEdge" imageset="ChipRacer" image="ButtonTopEdge" />
				<Image type="BottomEdge" imageset="ChipRacer" image="ButtonBottomEdge" />
				<Image type="TopLeftCorner" imageset="ChipRacer" image="ButtonTopLeftCorner" />
				<Image type="TopRightCorner" imageset="ChipRacer" image="ButtonTopRightCorner" />
				<Image type="BottomLeftCorner" imageset="ChipRacer" image="ButtonBottomLeftCorner" />
				<Image type="BottomRightCorner" imageset="ChipRacer" image="ButtonBottomRightCorner" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</FrameComponent>
		</ImagerySection>
		
		<ImagerySection name="frame_hover">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="LeftEdge" imageset="ChipRacer" image="ButtonLeftEdgeHover" />
				<Image type="RightEdge" imageset="ChipRacer" image="ButtonRightEdgeHover" />
				<Image type="TopEdge" imageset="ChipRacer" image="ButtonTopEdgeHover" />
				<Image type="BottomEdge" imageset="ChipRacer" image="ButtonBottomEdgeHover" />
				<Image type="TopLeftCorner" imageset="ChipRacer" image="ButtonTopLeftCornerHover" />
				<Image type="TopRightCorner" imageset="ChipRacer" image="ButtonTopRightCornerHover" />
				<Image type="BottomLeftCorner" imageset="ChipRacer" image="ButtonBottomLeftCornerHover" />
				<Image type="BottomRightCorner" imageset="ChipRacer" image="ButtonBottomRightCornerHover" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</FrameComponent>
		</ImagerySection>
		
		<ImagerySection name="frame_disabled">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="LeftEdge" imageset="ChipRacer" image="ButtonLeftEdgeDisabled" />
				<Image type="RightEdge" imageset="ChipRacer" image="ButtonRightEdgeDisabled" />
				<Image type="TopEdge" imageset="ChipRacer" image="ButtonTopEdgeDisabled" />
				<Image type="BottomEdge" imageset="ChipRacer" image="ButtonBottomEdgeDisabled" />
				<Image type="TopLeftCorner" imageset="ChipRacer" image="ButtonTopLeftCornerDisabled" />
				<Image type="TopRightCorner" imageset="ChipRacer" image="ButtonTopRightCornerDisabled" />
				<Image type="BottomLeftCorner" imageset="ChipRacer" image="ButtonBottomLeftCornerDisabled" />
				<Image type="BottomRightCorner" imageset="ChipRacer" image="ButtonBottomRightCornerDisabled" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</FrameComponent>
		</ImagerySection>

		<ImagerySection name="bg">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim imageset="ChipRacer" image="ButtonLeftEdge" dimension="Width" />
					</Dim>
					<Dim type="TopEdge">
						<ImageDim imageset="ChipRacer" image="ButtonTopEdge" dimension="Height" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" >
							<DimOperator op="Subtract" >
								<ImageDim imageset="ChipRacer" image="ButtonLeftEdge" dimension="Width" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonRightEdge" dimension="Width" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" >
							<DimOperator op="Subtract">
								<ImageDim imageset="ChipRacer" image="ButtonTopEdge" dimension="Height" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonBottomEdge" dimension="Height" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
				</Area>
				<Image imageset="ChipRacer" image="ButtonInner" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</ImageryComponent>
		</ImagerySection>
		
		<ImagerySection name="bg_hover">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim imageset="ChipRacer" image="ButtonLeftEdgeHover" dimension="Width" />
					</Dim>
					<Dim type="TopEdge">
						<ImageDim imageset="ChipRacer" image="ButtonTopEdgeHover" dimension="Height" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" >
							<DimOperator op="Subtract" >
								<ImageDim imageset="ChipRacer" image="ButtonLeftEdgeHover" dimension="Width" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonRightEdgeHover" dimension="Width" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" >
							<DimOperator op="Subtract">
								<ImageDim imageset="ChipRacer" image="ButtonTopEdgeHover" dimension="Height" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonBottomEdgeHover" dimension="Height" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
				</Area>
				<Image imageset="ChipRacer" image="ButtonInnerHover" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</ImageryComponent>
		</ImagerySection>
	
		<ImagerySection name="bg_disabled">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<ImageDim imageset="ChipRacer" image="ButtonLeftEdgeDisabled" dimension="Width" />
					</Dim>
					<Dim type="TopEdge">
						<ImageDim imageset="ChipRacer" image="ButtonTopEdgeDisabled" dimension="Height" />
					</Dim>
					<Dim type="Width">
						<UnifiedDim scale="1" type="Width" >
							<DimOperator op="Subtract" >
								<ImageDim imageset="ChipRacer" image="ButtonLeftEdgeDisabled" dimension="Width" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonRightEdgeDisabled" dimension="Width" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
					<Dim type="Height">
						<UnifiedDim scale="1" type="Height" >
							<DimOperator op="Subtract">
								<ImageDim imageset="ChipRacer" image="ButtonTopEdgeDisabled" dimension="Height" >
									<DimOperator op="Add" >
										<ImageDim imageset="ChipRacer" image="ButtonBottomEdgeDisabled" dimension="Height" />
									</DimOperator>
								</ImageDim>
							</DimOperator>
						</UnifiedDim>
					</Dim>
					
				</Area>
				<Image imageset="ChipRacer" image="ButtonInnerDisabled" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Stretched" />
			</ImageryComponent>
		</ImagerySection>
		
		<ImagerySection name="label">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" offset="0" type="RightEdge" />
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" offset="0" type="BottomEdge" />
					</Dim>
				</Area>
				<VertFormat type="CentreAligned" />
				<HorzFormat type="CentreAligned" />
			</TextComponent>
		</ImagerySection>
		
		<StateImagery name="Normal">
			<Layer>
				<Section section="bg" />
				<Section section="frame" />
				<Section section="label">
					<ColourProperty name="NormalTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="Hover">
			<Layer>
				<Section section="bg_hover" />
				<Section section="frame_hover" />
				<Section section="label">
					<ColourProperty name="HoverTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="Pushed">
			<Layer>
				<Section section="bg_hover" />
				<Section section="frame_hover" />
				<Section section="label">
					<ColourProperty name="PushedTextColour" />
				</Section>
			</Layer>
		</StateImagery>
		
		<StateImagery name="Disabled">
			<Layer>
				<Section section="bg_disabled" />
				<Section section="frame_disabled" />
				<Section section="label">
					<ColourProperty name="DisabledTextColour" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook> 
	
